    "# [Answer to Ex. 11.1.5]\n",
    "def f(X, y, W, eta):\n",
    "    errors = 0\n",
    "    \n",
    "    for xi, yi in zip(Xtrain, y):\n",
    "        bias = eta * (yi - predict(xi, W))\n",
    "        W[1:] = W[1:] + bias * xi\n",
    "        W[0] = W[0] + bias\n",
    "        errors = errors + int(bias != 0)\n",
    "    \n",
    "    return W, errors"
